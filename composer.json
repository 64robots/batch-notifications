{
    "name": "64robots/batch-notifications",
    "description": "A Laravel package that groups repetitive notifications in batches",
    "keywords": [
        "laravel",
        "batch",
        "notifications"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/notifications": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "R64\\BatchNotifications\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "R64\\BatchNotifications\\BatchNotificationsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
